  "description": "egg graphql",
  "private": true,
  "egg": {
    "typescript": true,
    "declarations": true
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-service",
    "stop": "egg-scripts stop --title=egg-server-service",
    "dev": "egg-bin dev",
