    "egg-cors": "^2.2.0",
    "egg-oauth2-server": "^2.2.6",
    "egg-passport": "^2.0.2",
    "egg-passport-github": "^1.0.0",
    "egg-scripts": "^2.6.0",
    "egg-sequelize": "^5.1.0",
    "egg-view-nunjucks": "^2.2.0",
    "mysql2": "^1.6.5",
    "pg": "^7.11.0",
    "pg-hstore": "^2.3.3"
