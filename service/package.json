    "autod": "autod",
    "lint": "tslint --project . -c tslint.json",
    "clean": "ets clean"
  },
  "dependencies": {
    "@switchdog/egg-graphql": "^3.2.0-alpha.1",
    "crypto": "^1.0.1",
    "dataloader": "^1.4.0",
